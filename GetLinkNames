#!/bin/bash

#######################################################
### Created by khan on Mon Oct 10 06:45:15 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: GetLinkNames <address>
#######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"; echo; exit 0
fi
#######################################################

lynx "$@" -accept_all_cookies -dump | sed '/References/q' |\
tr '\n' '|' | sed 's/\]| */\]/g' | tr '|' '\n' |\
tr '[' '\n[' | tr ']' '.' | grep '^[0-9]'
