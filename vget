#!/bin/bash

#######################################################
### Created by khan on Tue Jun 14 15:33:30 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: vget
#######################################################
if isHelp 0 $0 "$@"; then exit 0; fi

wget "$@" -o /tmp/wget.log
grep saved /tmp/wget.log | awk '{print $6}' | sed -e 's/^.//g' -e 's/.$//g'
