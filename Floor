#!/bin/bash

#######################################################
### Created by khan on Sun Jul 10 11:28:00 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: Floor
#######################################################
if isHelp 1 $0 "$@"; then exit 0; fi

num="$1"

if [[ `echo "$num % 1" | bc` != 0 ]]; then
	num=`echo "$num - 0.5" | bc -l`
fi

printf "%.0f\n" "$num"
