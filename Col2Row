#!/bin/bash

######################################################
### Created by khan on Fri Sep 9 14:55:03 CDT 2011 ###
######################################################

##################### Help Text #####################
# Usage: Col2Row
######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"; echo; exit 0
fi
######################################################

echo `cat /dev/stdin`
