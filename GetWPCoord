##################### Help Text #####################
# Usage: GetWPCoord

######################################################
if (( $# < 0 )) || [[ "$1" == --help ]]; then echo
sed -n "/^# Usa/,/^##*$/p" $0 | tr -d "#"; exit 0; fi
######################################################

## Initialize

## Functions

## Main
c=`cat /dev/stdin | grep -o '{{coord[^}]*' | sed 's,{{coord.,;,g'`
if [[ "$c" == "" ]]; then 
	c=";missing";
fi
echo -n "$c"
