#!/bin/bash

######################################################
### Created by khan on Tue Aug 9 13:33:30 CDT 2011 ###
######################################################

##################### Help Text #####################
# Usage: Log
######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"; echo; exit 0
fi
######################################################

logfile="$1"; shift

echo `date` - $@ >> $logfile

