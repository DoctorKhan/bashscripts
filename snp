651852
819147
819134
819171
234706
1801181
2298758
1979277
	27048"
}

## Main
if (($#==0)); then rsids; fi

while (($#>0)); do
	cp -f "$f1" "$f2"
	eval $1 > "$f3"
	shift
	paste "$f2" "$f3" > "$f1"
done
