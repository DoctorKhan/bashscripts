#!/bin/bash

#######################################################
### Created by khan on Mon Jul 4 20:19:12 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: Keycode <Symname>
#######################################################
if isHelp 1 $0 "$@"; then exit 0; fi

xmodmap -pke | grep -o "[0-9]* = $@" | cut -d ' ' -f 1
