#!/bin/bash

#######################################################
### Created by khan on Sun May 22 10:46:09 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: isInt <int>
#######################################################
if isHelp 0 $0 "$@"; then exit 0; fi

if [ ! $1 ]; then exit 1; fi
test "$1" -gt 0 2>/dev/null
if [ $? == 2 ]; then exit 1; else exit 0; fi

