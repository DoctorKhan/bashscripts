#!/bin/bash

######################################################
### Created by khan on Mon Aug 8 13:40:27 CDT 2011 ###
######################################################

##################### Help Text #####################
# Usage: notEmpty
######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"; echo; exit 0
fi
######################################################

if ! isEmpty $@; then 
exit 0
	else
	exit 1
	fi
