#!/bin/bash

#######################################################
### Created by khan on Sun Jun 22 17:58:19 CDT 2014 ###
#######################################################

###################### Help Text ######################
# Usage: GetN1

#######################################################
if (( $# < 0 )) || [[ "$1" == --help ]]; then echo
sed -n "/^# Usa/,/^##*$/p" $0 | tr -d "#"; exit 0; fi
#######################################################

## Initialize

## Functions

## Main
cut -f1 | while read line; do
	grep "$line" "$@" | grep -v "^$line"
done
