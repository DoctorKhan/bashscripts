#!/bin/bash

######################################################
### Created by khan on Sun Jan 1 10:18:48 CST 2012 ###
######################################################

##################### Help Text #####################
# Usage: Restore

######################################################
if (( $# < 0 )) || [[ "$1" == --help ]]; then echo
sed -n "/^# Usa/,/^##*$/p" $0 | tr -d "#"; exit 0; fi
######################################################

## Initialize

## Functions

## Main
for sFile in "$@"; do
	cp "`BackDir $sFile`" "`NormDir $sFile`"
done
