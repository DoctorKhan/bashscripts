#!/bin/bash

#######################################################
### Created by khan on Sun Jul 3 21:41:47 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: isNumeric
#######################################################
if isHelp 0 $0 "$@"; then exit 0; fi

if regmatch "$@" '[^0-9\.\+-]'; then exit 1; fi
