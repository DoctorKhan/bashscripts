#!/bin/bash

#######################################################
### Created by khan on Sun Jul 17 23:18:27 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: piped
#######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"
echo; exit 0; fi
#######################################################

while read line; do
	echo "$line"
done
