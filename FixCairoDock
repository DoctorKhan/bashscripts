#!/bin/bash

#######################################################
### Created by khan on Fri Sep 30 08:05:57 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: FixCairoDock
#######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"; echo; exit 0
fi
#######################################################

killall cairo-dock
cairo-dock -o &
