if (( $# < 0 )) || [[ "$1" == --help ]]; then echo
sed -n "/^# Usa/,/^##*$/p" $0 | tr -d "#"; exit 0; fi
#######################################################

## Initialize

## Functions

## Main
for file; do mv $file `echo $file | sed 's/^[^0-9]*0//g' | sed 's/E\([0-9][0-9]\).*\.\(.*\)/\1.\2/g'`; done

