#!/bin/bash

#######################################################
### Created by Khan on Sat Jul 16 12:35:06 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: StrTrim
#######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"
echo; exit 0; fi
#######################################################

sed 's/[^[:print:]]//g' | tr '\t' ' ' |\
sed -e 's/^ *//g' -e 's/ *$//g' -Ee 's/ +/ /g' |\
sed -e '/./,$!d' -e :a -e '/^\n*$/{$d;N;ba' -e '}'
