#!/bin/bash

#######################################################
### Created by khan on Tue Jul 19 02:07:11 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: ReadPrefs
#######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"
echo; exit 0; fi
#######################################################

sFile="$@"
cat "$sFile" | sed 's/^/-/g' | tr '\n' ' '
echo
