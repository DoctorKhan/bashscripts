#!/bin/bash

######################################################
### Created by khan on Wed Aug 3 21:36:59 CDT 2011 ###
######################################################

##################### Help Text #####################
# Usage: isDomain
######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"
echo; exit 0; fi
######################################################

exit $(nslookup -timeout=5 $@ | grep -ic find)

