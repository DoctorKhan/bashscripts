#!/bin/bash

#######################################################
### Created by khan on Wed Aug 10 12:39:34 CDT 2011 ###
#######################################################

###################### Help Text ######################
# Usage: RowDiff
#######################################################
if (( $# < 0 )) || [[ "$1" == *-h* ]]; then echo
	grep -A 99999 "# Help.*#*" $0 |\
	grep -E -m 2 -B 99999 "#{10,}"; echo; exit 0
fi
#######################################################

function GetInput()
{
	if (($# > 0)); then
		echo $@ | sed 's/\(.*\) * \-  *\(.*\)/\2;\1;/g' | tr ';' '\n'
	else
		cat /dev/stdin
	fi
}

GetInput $@ | tr -c '[0-9\n\-\.\+]' ' ' | transpose | awk '{print $2-$1}'
